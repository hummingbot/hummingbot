    "ts-react-toolbox": "^0.2.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "@types/react": "^16.8.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "keywords": [
    "react",
    "style",
    "css"
