{
  "name": "gateway-v2",
  "version": "0.1.9",
  "description": "Hummingbot Gateway",
  "main": "index.js",
  "license": "Apache-2.0",
  "repository": "https://github.com/coinalpha/gateway-v2",
  "scripts": {
    "prebuild": "rimraf dist && mkdir dist",
    "build": "tsc --project ./",
    "format": "prettier . --write",
    "lint": "eslint src test --format table --fix",
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --inspect src/index.ts",
    "start": "nodemon dist/index.js",
    "test": "jest --verbose false ./test-scripts/ethereum.controllers.test.ts",
    "test:unit": "jest --verbose ./test/",
    "test:cov": "jest --coverage ./test/",
    "test:scripts": "jest --verbose ./test-scripts/"
  },
  "dependencies": {
    "@ethersproject/experimental": "^5.3.0",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/sdk-core": "^3.0.0",
    "@uniswap/v3-core": "^1.0.0",
    "@uniswap/v3-periphery": "^1.1.1",
    "@uniswap/v3-sdk": "3.2.1",
    "abi-decoder": "^2.4.0",
    "app-root-path": "^3.0.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "ethers": "^5.4.1",
    "express": "^4.17.1",
    "express-ipfilter": "^1.2.0",
    "express-winston": "^4.1.0",
    "js-yaml": "^4.1.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/mathjs": "^9.4.2",
    "@types/node": "^15.12.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^27.1.0",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  }
}
